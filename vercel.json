{
  "buildCommand": "cd ai-trade-bot && npm run build",
  "devCommand": "cd ai-trade-bot && npm run dev",
  "installCommand": "cd ai-trade-bot && npm install",
  "outputDirectory": "ai-trade-bot/.next",
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/$1"
    }
  ]
}
